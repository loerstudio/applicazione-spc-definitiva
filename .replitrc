# Configurazione Replit per Expo - ZERO PROBLEMI
[env]
EXPO_USE_LOCAL_CLI = "1"
EXPO_NO_TELEMETRY = "1"
EXPO_NO_UPDATE_CHECK = "1"
NODE_ENV = "development"

[packager]
language = "nodejs"
entrypoint = "index.js"

[replit.config]
name = "SPC Fitness Coach"
run = "npm run start:lan"

[nix]
channel = "stable-22_11"